<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springcore.web.main.mapper.MainMapper">

	<!-- 테스트 쿼리 -->
	<select id="selectMainInfo" resultType="Map">
		/* MainMapper.selectMainInfo */
		SELECT
			LOGIN_ID
			 , PWD_HASH
			 , USER_NM
		FROM spring_dev.USERS
	</select>
	
</mapper>